{
  "name": "instastack-12factors",
  "version": "1.0.0",
  "description": "A sample app built with Node and React to illustrate the 12 factors",
  "main": "server/server.js",
  "repository": "https://github.com/schaable/instastack-12factors",
  "scripts": {
    "start": "yarn build && node ./dist/server.js",
    "postinstall": "yarn build",
    "build-client": "webpack -p",
    "build-server": "babel -d ./dist ./server -s",
    "build": "yarn build-client && yarn build-server",
    "prod": "node ./dist/server.js",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": ["12factors", "heroku", "sample-app"],
  "author": "Luis Schaab",
  "license": "ISC",
  "engines": {
    "node": "6.9.5",
    "yarn": "0.27.5"
  },
  "devDependencies": {
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "body-parser": "1.15.2",
    "css-loader": "0.25.0",
    "express": "4.14.0",
    "immutable": "3.8.1",
    "install": "0.8.1",
    "jest": "^20.0.4",
    "morgan": "1.7.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-hot-loader": "3.0.0-beta.5",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "redux": "3.6.0",
    "redux-saga": "0.12.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-merge": "0.14.1",
    "webpack-validator": "2.2.9"
  }
}
