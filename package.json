    "jest": "^20.0.4",
    "morgan": "1.7.0",
    "node-schedule": "^1.2.5",
    "path": "^0.12.7",
    "pg-promise": "^6.10.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-hot-loader": "^3.0.0",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "redux": "3.6.0",
    "redux-saga": "0.12.0",
    "style-loader": "^0.19.0",
