{
  "name": "instastack-12factors",
  "version": "1.0.0",
  "description": "A sample app built with Node and React to illustrate the 12 factors",
  "main": "server/index.js",
  "repository": "https://github.com/schaable/instastack-12factors",
  "scripts": {
    "dev": "yarn build && yarn start",
    "postinstall": "yarn build",
    "build-client": "webpack -p",
    "build-server": "babel -d ./dist ./server -s",
    "build": "yarn build-client && yarn build-server",
    "prestart": "gulp configFrontend",
    "start": "node ./dist/index.js",
    "worker": "node ./dist/worker.js",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "12factors",
    "heroku",
    "sample-app"
  ],
  "author": "Luis Schaab",
  "license": "ISC",
  "engines": {
    "node": "6.9.5",
    "yarn": "*"
  },
  "devDependencies": {
    "webpack-cli": "^1.3.9",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "bluebird": "^3.5.0",
    "body-parser": "1.15.2",
    "buffer-to-vinyl": "^1.1.0",
    "cool-ascii-faces": "^1.3.4",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "express": "4.14.0",
    "gulp": "^3.9.1",
    "imagemagick": "^0.1.3",
    "immutable": "3.8.1",
